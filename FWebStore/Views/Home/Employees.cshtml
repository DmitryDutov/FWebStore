@model IEnumerable<FWebStore.Models.Employee>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Employees</title>
</head>
<body>
<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Фамилия</th>
            <th>Имя</th>
            <th>Отчество</th>
            <th>Возраст</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.Id</td>
                <td>@employee.LastName</td>
                <td>@employee.FirstName</td>
                <td>@employee.Patronumic</td>
                <td>@employee.Age</td>
                <td>
                    <a href="~/home/employee/@employee.Id">Инфо.</a>
                </td>
            </tr>
        }
    </tbody>
</table>

@*Добавляем ссылку на главную страницу*@
<a href="~/">На главную</a>

</body>
</html>

